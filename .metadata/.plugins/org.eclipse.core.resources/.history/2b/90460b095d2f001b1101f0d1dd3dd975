package collection;

import java.util.*;

public class CollExample {

	

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		List<String> list = new ArrayList<String>();
		
		list.add("Bismark");
		list.add("Nana");
		list.add("Otu");
		list.add("Kwesi");
		
		
		int s = list.size();
		System.out.println(s);

	}

}
