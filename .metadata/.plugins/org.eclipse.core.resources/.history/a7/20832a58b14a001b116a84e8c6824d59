package trials;

public class Basics1 {
	

	int roll;
	String name, course;
	double fee;
	
	Basics1(int roll, String name, String course) {
		
		this.roll = roll;
		this.name = name;
		this.course = course;
		
	}
	
	Basics1(int roll, String name, String course, double fee) {
		
		this(roll, name, course);
		this.fee = fee;
	}
	
	public void display() {
		
		System.out.println(roll + " " + name + " " + course + " " + fee);
	}
	
	
	

}
